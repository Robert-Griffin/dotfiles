[core]
  editor = nvim
  excludesfile = ~/.gitignore
[user]
  name = Kenneth
  email = kennethlombardi@users.noreply.github.com
[push]
  default = simple
[pull]
  rebase = true
[color]
  ui = true
[difftool "sourcetree"]
  cmd = opendiff \"$LOCAL\" \"$REMOTE\"
  path =
[mergetool "sourcetree"]
  cmd = /Users/kenl/Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
  trustExitCode = true
[commit]
  gpgsign = false
